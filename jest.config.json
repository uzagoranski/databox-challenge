{
  "roots": ["./"],
  "coverageDirectory": "../coverage",
  "collectCoverage": true,
  "coveragePathIgnorePatterns": ["/node_modules/", "/dist", ".mock.ts", ".module.ts", ".dto.ts", ".entity.ts", ".response.ts", ".type.ts", "generate-swagger.ts", "main.ts", "config.ts", ".d.ts", ".enum.ts"],
  "moduleFileExtensions": ["ts", "tsx", "js", "jsx", "json", "node"],
  "rootDir": "./",
  "testRegex": ".test.ts$",
  "transform": {
    "^.+\\.(t|j)s$": "@swc/jest"
  },
  "testEnvironment": "node",
  "testTimeout": 15000
}
